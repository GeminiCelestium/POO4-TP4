@model ModernRecrut.MVC.Areas.Identity.Data.ModernRecrutMVCUser

@{
    ViewData["Title"] = $"Liste des documents";
}

<h1>Liste des documents</h1>

<form asp-action="Details">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="Id" class="control-label"></label>
        <select asp-for="Id" class="form-control" asp-items="ViewBag.users"></select>
        <span class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Voir les documents" class="btn btn-primary" />
    </div>
</form>
